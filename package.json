{
    "name": "altv-athena",
    "version": "1.0.6",
    "description": "A Roleplay project for alt:V.",
    "scripts": {
        "build": "npm run clean && tsc && npm run copy",
        "copy": "copyfiles \"src/**/*\" --exclude=**/*.ts --up 1 ./resources",
        "clean": "rimraf resources",
        "update": "altv-pkg d release && git pull origin master && npm install && git submodule update --force --init --recursive --remote",
        "windows": "npm run build && altv-server.exe",
        "linux": "npm run build && ./start.sh"
    },
    "author": "stuyk",
    "license": "ISC",
    "devDependencies": {
        "@altv/types-client": "^1.4.6",
        "@altv/types-natives": "^1.1.0",
        "@altv/types-server": "^1.6.5",
        "@altv/types-webview": "^1.0.2",
        "@types/axios": "^0.14.0",
        "@types/express": "^4.17.8",
        "@types/gulp": "^4.0.6",
        "@types/gulp-debug": "^2.0.31",
        "@types/node": "^14.6.1",
        "altv-pkg": "latest",
        "copyfiles": "^2.3.0",
        "rimraf": "^3.0.2",
        "typescript": "^4.0.2"
    },
    "type": "module",
    "dependencies": {
        "axios": "^0.21.1",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "elliptic": "^6.5.3",
        "express": "^4.17.1",
        "public-ip": "^4.0.3",
        "simplymongo": "^2.0.8",
        "sjcl": "^1.0.8"
    }
}
