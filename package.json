{
    "name": "altv-athena",
    "version": "2.0.3",
    "description": "a roleplay framework for alt:V",
    "author": "stuyk",
    "type": "module",
    "scripts": {
        "[-] Server Deployment Commands": "",
        "windows": "npm run build && npm run vue-build && npm-run-all --parallel athena-webserver athena-streamer windows-server",
        "linux": "npm run build && npm run vue-build && npm-run-all --parallel athena-webserver athena-streamer linux-server",
        "dev": "npm run build && npm-run-all --parallel athena-webserver athena-streamer athena-devmode",
        "[-] Updating, Installing, Cleanup": "",
        "build": "npm run clean && swc ./src -d ./resources --copy-files --only **/*.ts --quiet",
        "clean": "rimraf ./resources",
        "update": "altv-pkg d release",
        "check": "tsc",
        "[-] Binary Running Commands": "",
        "windows-server": "altv-server.exe",
        "linux-server": "./start.sh",
        "[-] Parallel Programs": "",
        "athena-streamer": "node ./resources/streamer/index.js",
        "athena-webserver": "node ./resources/webserver/index.js",
        "athena-devmode": "nodemon --watch \"./src/**\" --ext \"ts\" --exec \"npm run build && altv-server.exe\"",
        "[-] Vue WebView Deployment": "",
        "vue-dev": "vite ./src-webviews",
        "vue-build": "vue-tsc --noEmit -p ./src-webviews/tsconfig.json && vite build ./src-webviews --outDir=\"../resources/webviews\" --emptyOutDir",
        "vue-serve": "vite preview ./src-webviews"
    },
    "devDependencies": {
        "@altv/types-client": "^1.8.0",
        "@altv/types-natives": "^1.4.2",
        "@altv/types-server": "^2.0.0",
        "@altv/types-shared": "^1.0.3",
        "@altv/types-webview": "^1.0.2",
        "@swc/cli": "^0.1.46",
        "@swc/core": "^1.2.60",
        "@types/axios": "^0.14.0",
        "@types/express": "^4.17.8",
        "@types/glob": "^7.1.3",
        "@types/mongodb": "^3.6.8",
        "@types/node": "^14.6.1",
        "@types/node-fetch": "^2.5.10",
        "@types/ps-node": "^0.1.0",
        "@types/sockjs": "^0.3.33",
        "@types/sockjs-client": "^1.5.1",
        "@vitejs/plugin-vue": "^1.3.0",
        "@vue/compiler-sfc": "^3.0.5",
        "altv-pkg": "^1.2.0",
        "dotenv": "^10.0.0",
        "fs-extra": "^9.1.0",
        "glob": "^7.1.6",
        "nodemon": "^2.0.7",
        "npm-run-all": "^4.1.5",
        "promisify-child-process": "^4.1.1",
        "rimraf": "^3.0.2",
        "typescript": "latest",
        "vite": "^2.4.4",
        "vue": "^3.0.5",
        "vue-tsc": "^0.2.2"
    },
    "dependencies": {
        "@stuyk/ezmongodb": "^1.3.0",
        "axios": "^0.21.1",
        "body-parser": "^1.19.0",
        "chalk": "^4.1.0",
        "cors": "^2.8.5",
        "discord.js": "^12.5.3",
        "elliptic": "^6.5.4",
        "express": "^4.17.1",
        "node-fetch": "^2.6.1",
        "ps-node": "^0.1.6",
        "sjcl": "^1.0.8",
        "sockjs": "^0.3.21",
        "sockjs-client": "^1.5.1"
    },
    "prettier": {
        "tabWidth": 4,
        "printWidth": 120,
        "singleQuote": true,
        "semi": true,
        "arrowParens": "always",
        "trailingComma": "none"
    }
}
