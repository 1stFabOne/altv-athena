{
    "name": "altv-athena",
    "version": "2.0.4",
    "description": "a roleplay framework for alt:V",
    "author": "stuyk",
    "type": "module",
    "scripts": {
        "[-] Server Deployment Commands (They All Do Different Things)": "",
        "windows": "npm run build && npm run vue-build && npm-run-all --parallel athena-webserver athena-streamer windows-server",
        "linux": "npm run build && npm run vue-build && npm-run-all --parallel athena-webserver athena-streamer linux-server",
        "dev": "npm run build && npm-run-all --parallel vue-dev athena-webserver athena-streamer athena-devmode",
        "devtest": "npm run build && npm-run-all --parallel athena-webserver athena-streamer athena-devtest",
        "[-] Updating, Installing, Cleanup": "",
        "build": "npm run clean && npx swc ./src -d ./resources --copy-files --only **/*.ts --quiet",
        "clean": "node ./scripts/rimraf/index.js",
        "update": "npx altv-pkg d release",
        "check": "tsc",
        "[-] Binary Running Commands": "",
        "windows-server": "npm run cfg-prod && altv-server.exe",
        "linux-server": "npm run cfg-prod && ./start.sh",
        "[-] Parallel Programs": "",
        "athena-streamer": "node ./resources/streamer/index.js",
        "athena-webserver": "node ./resources/webserver/index.js",
        "athena-devtest": "npx nodemon --watch \"./src/**\" --ext \"ts\" --exec \"npm run build && npm run vue-build && npm run cfg-devtest && altv-server.exe\"",
        "athena-devmode": "npx nodemon --watch \"./src/**\" --ext \"ts\" --exec \"npm run build && npm run cfg-dev && altv-server.exe\"",
        "[-] Vue WebView Deployment": "",
        "vue-dev": "npx vite ./src-webviews --clearScreen=false --debug=false --logLevel=silent",
        "vue-build": "npx vue-tsc --noEmit -p ./src-webviews/tsconfig.json && npx vite build ./src-webviews",
        "vue-serve": "npx vite preview ./src-webviews",
        "[-] Tools": "",
        "hwid-win": "cd tools && altv-athena-hwid-win.exe",
        "hwid-linux": "cd tools && altv-athena-hwid-linux",
        "[-] Configs - Create a list below": "",
        "cfg-dev": "npx altv-config ./configs/dev.json",
        "cfg-devtest": "npx altv-config ./configs/devtest.json",
        "cfg-prod": "npx altv-config ./configs/prod.json"
    },
    "devDependencies": {
        "@altv/types-client": "^1.9.7",
        "@altv/types-natives": "^1.4.3",
        "@altv/types-server": "^2.1.6",
        "@altv/types-shared": "^1.1.1",
        "@altv/types-webview": "^1.0.2",
        "@stuyk/altv-config": "^1.0.2",
        "@swc/cli": "^0.1.50",
        "@swc/core": "^1.2.90",
        "@types/axios": "^0.14.0",
        "@types/express": "^4.17.8",
        "@types/glob": "^7.1.3",
        "@types/mongodb": "^3.6.8",
        "@types/node": "^14.6.1",
        "@types/node-fetch": "^2.5.10",
        "@types/ps-node": "^0.1.0",
        "@types/sockjs": "^0.3.33",
        "@types/sockjs-client": "^1.5.1",
        "@vitejs/plugin-vue": "^1.3.0",
        "@vue/compiler-sfc": "^3.0.5",
        "altv-pkg": "^1.2.0",
        "fs-extra": "^9.1.0",
        "glob": "^7.2.0",
        "nodemon": "^2.0.14",
        "npm-run-all": "^4.1.5",
        "promisify-child-process": "^4.1.1",
        "typescript": "latest",
        "vite": "^2.6.13",
        "vue": "^3.0.5",
        "vue-resize-text": "^0.1.1",
        "vue-tsc": "^0.28.10"
    },
    "dependencies": {
        "@stuyk/ezmongodb": "^1.4.0",
        "axios": "^0.24.0",
        "body-parser": "^1.19.0",
        "chalk": "^4.1.2",
        "cors": "^2.8.5",
        "discord.js": "^12.5.3",
        "elliptic": "^6.5.4",
        "express": "^4.17.1",
        "node-fetch": "^3.0.0",
        "ps-node": "^0.1.6",
        "sjcl": "^1.0.8",
        "sockjs": "^0.3.21",
        "sockjs-client": "^1.5.2"
    },
    "prettier": {
        "tabWidth": 4,
        "printWidth": 120,
        "singleQuote": true,
        "semi": true,
        "arrowParens": "always",
        "trailingComma": "all"
    }
}
